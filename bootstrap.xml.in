<schemaPathReplacements>
  <replace old="[CONFIGDIR]" new="&configDir;"/>
  <replace old="[SCHEMAPATH]" new="&schemaDir;"/>
  <replace old="[SCHEMAPATH_IMPORTS]" new="&schemaDir;"/>
